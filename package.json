{
  "name": "cars",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.1",
    "image-compressor": "^1.2.0",
    "jquery": "^3.4.1",
    "js-base64": "^2.5.1",
    "md5": "^2.2.1",
    "mint-ui": "^2.2.13",
    "qs": "^6.6.0",
    "semver": "^6.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "upload-to-ali": "^6.0.0",
    "vant": "^2.1.8",
    "vconsole": "^3.3.0",
    "vue": "^2.6.6",
    "vue-awesome-swiper": "^3.1.3",
    "vue-baidu-map": "^0.21.16",
    "vue-carousel-3d": "^0.1.23",
    "vue-echarts": "^4.0.3",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.1",
    "vue-video-play": "^7.0.4",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "vue-video-player": "^5.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/html-end-tags": "error",
      "vue/require-default-prop": "error",
      "vue/require-prop-types": "error",
      "vue/require-v-for-key": 1,
      "vue/singleline-html-element-content-newline": 0,
      "vue/order-in-components": 0,
      "vue/html-closing-bracket-spacing": 0,
      "no-unused-vars": 0,
      "eqeqeq": 0,
      "indent": 0,
      "vue/no-unused-components": 0,
      "no-multi-spaces": 0,
      "no-empty-function": 0,
      "no-undef": 1,
      "handle-callback-err": 0,
      "vue/no-v-html": 0,
      "vue/max-attributes-per-line": 0,
      "semi": 0,
      "space-before-function-paren": 0,
      "no-trailing-spaces": 0,
      "quotes": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "pre-commit": [
    "lint"
  ]
}
